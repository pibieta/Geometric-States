@article{Takeuchi,
	abstract = "Hopf algebras in braided tensor categories are studied with emphasis on finite (i.e., rigid) Hopf algebras. By imitating Larson and Sweedler's (1969) Hopf module construction, it is proved that the antipode of a finite Hopf algebra is an isomorphism of the category and that the left or right integral space is an invertible object. Diagrammatic methods are effectively used throughout the arguments.",
	author = "Takeuchi, Mitsuhiro",
	doi = "10.1016/S0022-4049(97)00207-7",
